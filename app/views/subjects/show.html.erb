<h1><%= @subject.name %></h1>
<% if @levels.empty?%>
  <p>No subject avaliable</p>
<% else %>
  <table>
  <tbody>  
    <% @levels.each do |l| %>
    <tr>  
      <td><%= l.level_name %></td>
      <td><%= l.level %></td>
      <td><%= link_to "Delete", subject_level_path(l), method: :delete %></td>
    </tr>
    <% end %>
  </tbody>
  </table>
<% end %>
<%= link_to "New subject", new_subject_level_path %>