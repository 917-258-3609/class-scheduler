<h3><%= "#{@teacher.name}" %></h3>
<% @teacher.subject_levels.each do |l| %>
    <p><%= l.to_s %></p>
<% end %>
<%= link_to "All teacher", teachers_path %>

<%= week_calendar(events: @calendar_occurrences) do |date, occurs| %>
  <h6><%= date.day %></h6>
  <% occurs.each do |o| %>
    <%= o.start_time.strftime("%H:%M -") + o.end_time.strftime(" %H:%M") %>
    <br/>
  <% end %>
<% end %>

<div class="content-body">
</div>